{
  "name": "@delta-comic/ui",
  "version": "0.0.1",
  "description": "空阙虱楼",
  "homepage": "https://github.com/delta-comic/delta-comic-core",
  "license": "AGPL-3.0-only",
  "author": {
    "name": "wenxig",
    "email": "wenxinguo12@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delta-comic/delta-comic-core.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/index2.js",
      "require": "./dist/index2.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build && pnpm pack --out ./dist/pack.tgz",
    "version:major": "pnpm version major --no-git-tag-version",
    "version:minor": "pnpm version minor --no-git-tag-version",
    "version:patch": "pnpm version patch --no-git-tag-version"
  },
  "dependencies": {
    "@lhlyu/vue-virtual-waterfall": "1.0.8",
    "@vant/use": "^1.6.0",
    "@vueuse/core": "catalog:",
    "dayjs": "catalog:",
    "mitt": "catalog:",
    "motion-v": "1.10.3",
    "tailwindcss-safe-area-capacitor": "^0.5.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "browserslist": "^4.28.1",
    "lightningcss": "^1.30.2",
    "oxfmt": "^0.32.0",
    "tailwindcss": "^4.1.18",
    "unplugin-vue-components": "31.0.0",
    "vue-component-type-helpers": "^3.2.4"
  },
  "peerDependencies": {
    "@delta-comic/core": "workspace:*",
    "@delta-comic/model": "workspace:*",
    "@delta-comic/utils": "workspace:*",
    "naive-ui": "^2.43",
    "pinia": "^3.0",
    "vant": "^4.9",
    "vue": "^3.5",
    "vue-router": "^5.0"
  },
  "release": {
    "tagFormat": "ui-${version}"
  },
  "dist": {
    "tarball": "./dist/pack.tgz"
  },
  "readme": "./README.md"
}